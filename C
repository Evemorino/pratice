int main(){
	int max(int a,int b);
	int num1,num2,num3;
	scanf("%d %d",&num1,&num2);
	num3=max(num1,num2);
	printf("%d",num3);
	return 0;
}

int max(int a,int b){
	if(a>b){
		return a;
	}else{
		return b;
	}
}

	int a=0,b=0;
	printf("请输入："); 
	scanf("%d,%d",&a,&b);
	if(b==0){
		printf("除数不能是0\n");
	}
		printf("结果是%d\n",a/b);
		return 0;
    
    
