int main(){
	int a=0;
	switch(a){
		case 1:
		printf("这是第一个\n");
		return 0;
		case 2:

printf("这是第二个\n");
		return 0;
		default:
		printf("这是剩余的\n");
		return 0;
		case 3:
		printf("这是第三个\n");
		return 0;

	}
}


