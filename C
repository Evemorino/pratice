int main()
{
	int price=0;
	int bill=0;
	scanf("%d",&price);
	scanf("%d",&bill);
	if(price>bill){
		printf("money is not enough\n");
	}else{
		int charge=bill-price;
		printf("here is your charge%d\n",charge);
	}
}

